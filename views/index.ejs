<html>
<style type="text/css">
html, body, {
    position:fixed;
    top:0;
    bottom:0;
    left:0;
    right:0;
}
</style>
<head>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="ejs.js"></script>
    <link rel="stylesheet" href="jquery-ui.min.css">
	<script src="jquery.js"></script>
	<script src="jquery-ui.min.js"></script>
    <link href="tabulator.css" rel="stylesheet">
    <script type="text/javascript" src="tabulator.js"></script>
    <link rel="stylesheet" type="text/css" href="globalstyle.css">
</head>

<body>
    <pre><%= jsondata %></pre>
    <div id="main-table" ></div>
</body>

<script>
    //run script once the document is fully loaded
    $(document).ready(function() {
        $("#main-table").tabulator({
    height:"320px", // set height of table (optional)
    fitColumns:true, //fit columns to width of table (optional)
    columns:[ //Define Table Columns
        {title:"Issue", field:"issue", sorter:"string"},
        {title:"Progress", field:"prog", sorter:"number", align:"left", formatter:"progress"},
        {title:"Description", field:"des", sorter:"string", sortable:false},
        {title:"Assigned To", field:"assign", sorter:"string", align:"center"},
    ],
    rowClick:function(e, id, data, row){ //trigger an alert message when the row is clicked
        alert("Row " + id + " Clicked");
    },
});
        //define some sample data
var tabledata = [
    {id:1, issue:"Panorama- Reports INV- RO0290 All Vaccine Inventory Report Months remaining is incorrect for Total by Catalogue Item (DEFECT)", prog:"100", des:"Description for description", assign:"Person Assigned To"},
    
];

//load sample data into the table
$("#main-table").tabulator("setData", tabledata);
    });
</script>

</html>